{
  "hosting": {
    "public": "/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
    // Drops `.html` from uploaded URLs, elimina de las urls .html
    "cleanUrls": true
  }
}
